deploy	: html
	runghc ifphsite.hs deploy

preview : html
	runghc ifphsite.hs preview

html	: md
	runghc ifphsite.hs build

md	: $(DWNS)

%.markdown	: %.lhs
	runghc lhs2md.hs `date +%F` < $< > $@

clean	:
	-rm *~
